{
  "name": "Everyday Dog",
  "version": "1.0",
  "description": "You will see a beautiful dog everytime you open a new tab!!",
  "permissions": ["activeTab", "tabs", "storage"],

  "chrome_url_overrides" : {
    "newtab": "index.html"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  
  "manifest_version": 2
}
