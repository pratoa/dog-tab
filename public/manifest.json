{
  "name": "DogTab",
  "version": "1.0",
  "description": "You will see a beautiful dog everytime you open a new tab!!",
  "permissions": ["activeTab", "tabs", "storage", "topSites"],
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png",
    "192": "favicon-192x192.png" 
  },
  "manifest_version": 2
}
