{
  "name": "DogTab",
  "version": "1.0.1",
  "description": "DogTab",
  "permissions": ["storage", "topSites"],
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png",
    "128": "favicon-128x128.png",
    "192": "favicon-192x192.png" 
  },
  "manifest_version": 2
}
